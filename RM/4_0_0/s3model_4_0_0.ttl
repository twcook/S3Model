        sh:datatype s3m:XdFile ;
        sh:maxCount 1 ;
        sh:minCount 0 ;
        ] ;

    sh:property [
        sh:path s3m:proof ;
        rdf:type sh:PropertyShape;
        sh:name "Proof" ;
        sh:description "Proof of attestation such as an GPG signature."@en ;
        sh:datatype xsd:XdFile ;
