        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:minCount 0 ;
        ] ;

    sh:property [
        sh:path s3m:partyRef ;
        rdf:type sh:PropertyShape;
        sh:name "Party Ref" ;
