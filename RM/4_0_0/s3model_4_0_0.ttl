        sh:datatype s3m:Cluster ;
        sh:maxCount 1 ;
        sh:minCount 0 ;
        ] ;

    sh:property [
        sh:path s3m:timestamp ;
        rdf:type sh:PropertyShape;
        sh:name "Time Stamp" ;
