        sh:minCount 0 ;
        ] ;

    sh:property [
        sh:path s3m:end ;
        rdf:type sh:PropertyShape;
        sh:name "End DateTime" ;
        sh:description "The ending datetime when the participation took place."@en ;
        sh:datatype xsd:dateTime ;
        sh:maxCount 1 ;
        sh:minCount 0 ;
        ] .

